{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["standard", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "space-before-function-paren": 0,
    "react/react-in-jsx-scope": "off"
    /* "eslint import/no-absolute-path": "[2, { commonjs: false, amd: true }]" */
  }
}
